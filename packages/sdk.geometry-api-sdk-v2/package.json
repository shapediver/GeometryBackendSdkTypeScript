{
  "name": "@shapediver/sdk.geometry-api-sdk-v2",
  "version": "1.0.12",
  "description": "SDK to communicate with the Geometry View API version 2",
  "keywords": [
    "shapediver",
    "sdk",
    "geometry api",
    "mgmt"
  ],
  "author": "Matthias Reisacher <matt@shapediver.com>",
  "license": "ISC",
  "main": "dist/index.js",
  "directories": {
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/shapediver"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shapediver/GeometryBackendSdkTypeScript.git"
  },
  "scripts": {
    "test": "bash ../../scripts/test.sh",
    "check": "tsc --noEmit",
    "build": "bash ../../scripts/build.sh",
    "build-dep": "bash ../../scripts/build-dep.sh"
  },
  "bugs": {
    "url": "https://github.com/shapediver/GeometryBackendSdkTypeScript/issues"
  },
  "homepage": "https://github.com/shapediver/GeometryBackendSdkTypeScript#readme",
  "typings": "dist/index.d.ts",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testTimeout": 60000,
    "setupFilesAfterEnv": [
      "./test.config.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  },
  "devDependencies": {
    "jest": "^27.4.5",
    "lerna": "3.22.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  },
  "dependencies": {
    "@shapediver/sdk.geometry-api-sdk-core": "^1.0.4",
    "@shapediver/api.geometry-api-dto-v2": "^1.1.4"
  }
}
